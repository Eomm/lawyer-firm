{
  "name": "studio-legale-ferrini",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "start:prod": "npm run clean && npm run build && npm run preview",
    "clean": "rm -rf dist/",
    "build": "npm run lint && astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "eslint . --ext .js,.mjs,.cjs,.ts,.jsx,.tsx,.astro",
    "lint:fix": "eslint . --ext .js,.mjs,.cjs,.ts,.jsx,.tsx,.astro --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "@astrojs/check": "^0.9.6",
    "@astrojs/sitemap": "^3.6.0",
    "@tailwindcss/vite": "^4.1.18",
    "astro": "^5.16.6",
    "lucide-astro": "^0.539.0",
    "tailwindcss": "^4.1.12",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/node": "^22.19.3",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-astro": "^1.5.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.7.4",
    "prettier-plugin-astro": "^0.14.1",
    "typescript-eslint": "^8.51.0"
  }
}
